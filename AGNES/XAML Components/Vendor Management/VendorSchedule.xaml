                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Thursday" Margin="-3,0,0,0" Background="Black" Foreground="White" FontSize="24" TextAlignment="Center"/>
