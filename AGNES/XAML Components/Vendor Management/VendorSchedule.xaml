<Window x:Class="VendorSchedule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AGNES"
        mc:Ignorable="d"
        Title="Vendor Scheduling" Height="800" Width="1280" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Icon="/AGNES;component/Resources/Vendors.png">
    <Window.Effect>
        <DropShadowEffect Opacity="0.7" BlurRadius="14" ShadowDepth="6" Direction="240"/>
    </Window.Effect>
    <Grid Height="41" VerticalAlignment="Bottom">
        <ToolBar x:Name="tlbVendors" HorizontalAlignment="Left" Height="58" Margin="0,-725,0,0" VerticalAlignment="Top" Width="1274">
            <Image Height="41" VerticalAlignment="Top" Width="70" Source="/AGNES;component/Resources/LoadPrevious.png" ToolTip="Import Previous Week" Margin="0,5,0,0" />
            <Image Height="41" VerticalAlignment="Top" Width="70" Source="/AGNES;component/Resources/save.png" ToolTip="Save Schedule" Margin="0,5,0,0" />
            <Separator />
            <Image Height="41" VerticalAlignment="Top" Width="70" Source="/AGNES;component/Resources/brands.png" ToolTip="Show Local Brands" Margin="0,5,0,0" />
            <Image Height="41" VerticalAlignment="Top" Width="70" Source="/AGNES;component/Resources/FoodTruck.png" ToolTip="Show Food Trucks" Margin="0,5,0,0" />
            <Separator />
            
        </ToolBar>
        <DockPanel HorizontalAlignment="Left" Height="657" LastChildFill="False" Margin="10,-662,0,0" VerticalAlignment="Top" Width="1254">
            <Border BorderBrush="Black" BorderThickness="1,1,1,1">
                <StackPanel x:Name="stkVendors" Height="677" VerticalAlignment="Top" Width="209">
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Vendor" FontSize="24" TextAlignment="Center" Background="Black" Foreground="White"/>
                    </Border>
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Vendor" FontSize="24" TextAlignment="Center"/>
                    </Border>
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Vendor" FontSize="24" TextAlignment="Center"/>
                    </Border>
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
