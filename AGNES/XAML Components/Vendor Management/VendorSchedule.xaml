        <StatusBar x:Name="barSaveStatus" Height="35" VerticalAlignment="Bottom" Background="#FF19F30E">
            <TextBlock x:Name="tbSaveStatus" Text="Saved" Width="1268" TextAlignment="Center" FontWeight="SemiBold" FontSize="24" />
