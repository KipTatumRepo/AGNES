                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Vendor" FontSize="24" TextAlignment="Center"/>
                    </Border>
                </StackPanel>
            </Border>
            <StackPanel Height="677" VerticalAlignment="Top" Width="10"/>
            <Border x:Name="brdFri" BorderBrush="Black" BorderThickness="1,1,1,1">
                <StackPanel Height="677" VerticalAlignment="Top" Width="207">
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
                        <TextBlock TextWrapping="Wrap" Text="Friday" Margin="-3,0,0,0" Background="Black" Foreground="White"  FontSize="24" TextAlignment="Center"/>
                    </Border>
                    <Border BorderBrush="Black" BorderThickness="1,1,1,1" Padding="3,0,0,0">
